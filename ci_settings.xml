<settings>
    <servers>
        <server>
            <id>gitlab-maven</id>
            <configuration>
                <httpHeaders>
                    <property>
                        <name>Job-Token</name>
                        <value>${env.CI_JOB_TOKEN}</value>
                    </property>
                </httpHeaders>
            </configuration>
        </server>
        <server>
            <id>ftp-repo</id>
            <username>${env.MVN_REPO_USER}</username>
            <password>${env.MVN_REPO_PASS}</password>
        </server>
    </servers>
